# Hoover-Simulation
public class Customer {

private int age;
private int time;
pricate String name;
private char gender;

public Customer (int age, int time, String name, char gender) {
  this.age = age;
  this.time = time;
  this.name = name;
  this.gender = gender;
}

public String getName () {
  return name;
}

public int getTime () {
  return time;
}

public int getAge () {
  return age;
}

public char getGender () {
  return gender;
}

@Override

public String toString () {
  return "Customer: " + "Name: " + name + "Age: " + age + "Time: " +    time + "gender: " + gender;
 }
}
